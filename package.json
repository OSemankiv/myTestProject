  "main": "server.js",
  "scripts": {
    "dev": "nodemon app/bin/server.js",
    "db:create": "./node_modules/.bin/sequelize db:create --config  app/config/db.js",
    "db:migrate": "node_modules/.bin/sequelize  db:migrate --config  app/config/db.js  --migrations-path app/database/migrations ",
    "db:migrate:undo": "node_modules/.bin/sequelize  db:migrate:undo --config  app/config/db.js  --migrations-path app/database/migrations ",
    "db:migrate:undo:all": "node_modules/.bin/sequelize  db:migrate:undo:all --config  app/config/db.js  --migrations-path app/database/migrations ",
    "db:migrate:status": "node_modules/.bin/sequelize  db:migrate:status --config  app/config/db.js  --migrations-path app/database/migrations ",
    "db:seed:all": "node_modules/.bin/sequelize  db:seed:all --config  app/config/db.js  --seeders-path app/database/seeders ",
    "db:seed:undo": "node_modules/.bin/sequelize  db:seed:undo --config  app/config/db.js  --seeders-path app/database/seeders ",
    "db:seed:undo:all": "node_modules/.bin/sequelize  db:seed:undo:all --config  app/config/db.js  --seeders-path app/database/seeders "
  },
